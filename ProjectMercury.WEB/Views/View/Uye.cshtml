                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="text-primary">Üye Adı Soyadı</label>
                                <input id="name" type="text" value="@Model.UyeAdiSoyadi" placeholder="Adı Soyadı" name="UyeAdiSoyadi" class="form-control input-md" required>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="text-primary">Üye E-Mail</label>
                                <input id="email" type="text" value="@Model.MailAdresi" placeholder="Email" name="MailAdresi" class="form-control input-md" required email>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="text-primary">Üye Şifre</label>
                                <input id="email" type="text" value="@Model.Sifre" placeholder="Şifre" name="Sifre" class="form-control input-md" required>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="text-primary">Üye Telefon</label>
                                <input id="phone" type="tel" value="@Model.Telefon" placeholder="Telefon" name="Telefon" class="form-control input-md" required>
                            </div>
                        </div>

                        <div class="form-group col-sm-6">
                            <label class="text-primary">Üye Adres</label>
                            <input id="billingAddress" type="text" value="@Model.Adres" placeholder="Adres" name="Adres" class="form-control input-md" required>
                        </div>
                        <input name="UyelerID" value="@Model.UyelerID" hidden="hidden">
                        <input name="Gorev" value="Guncelle" hidden="hidden">
                        <div class="form-group col-sm-3">
                            <label class="text-primary">Bilgileri Güncelle</label>
                            <button class="btn btn-success form-control">Kaydet</button>
                        </div>
